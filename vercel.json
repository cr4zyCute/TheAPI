{
  "version": 2,
  "builds": [
    {
      "src": "servers.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/channels.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/channels.json",
      "dest": "/public/channels.json"
    },
    {
      "src": "/(.*)",
      "dest": "/servers.js"
    }
  ]
}